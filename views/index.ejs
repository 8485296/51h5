    #cover{
      background:#000;
      opacity: 0.4;
      z-index: 90;
      display: none;
      position: absolute;
      left: 0;
      top: 0;
      bottom: 0;
      right: 0;
      height: 100%;
      width: 100%;
    }
  </style>
</head>
<body>
    <div id="dowebok">
        <div class="section">
            <div id="loading">
                <img src="http://51wzn.oss-cn-beijing.aliyuncs.com/loading_new.gif">
                <div class="countdown" data-time="3" data-value="0">
            
                </div>
              </div>
              <video id="video" preload="auto" x-webkit-airplay="true" webkit-playsinline="true" playsinline="true" style="width: 100%; height: 100%;">
                    <source id="mp4" src="http://51wzn.oss-cn-beijing.aliyuncs.com/%E6%80%BB-%E5%90%88%E6%88%90.mp4" type="video/mp4">
              </video>
            
              <audio id="loading-mp3" autoplay="true" loop="loop" src="http://51wzn.oss-cn-beijing.aliyuncs.com/BGM_loading.mp3"></audio>
              <audio id="loop-mp3" autoplay="true" loop="loop" src="http://51wzn.oss-cn-beijing.aliyuncs.com/BGM_loop.mp3"></audio>
              
              <div id="gif">
                <img src="http://51wzn.oss-cn-beijing.aliyuncs.com/todown.gif">
              </div> 
        </div>
        <div class="section">
            <div id="card">
                
            </div>
            <div id="invitation">
                <img src="/images/yaoqinghan.png" >
                <div id="name">
                  
                </div>
            </div>
            <div id="bt">

            </div>
            <div id="cover"></div>
        </div>
    </div>
<script>

function getSearchString(key) {
     // 获取URL中?之后的字符
      var str = location.search;
      str = str.substring(1,str.length);
      
      // 以&分隔字符串，获得类似name=xiaoli这样的元素数组
      var arr = str.split("&");
      var obj = new Object();
      
     // 将每一个数组元素以=分隔并赋给obj对象
     for(var i = 0; i < arr.length; i++) {
         var tmp_arr = arr[i].split("=");
         obj[decodeURIComponent(tmp_arr[0])] = decodeURIComponent(tmp_arr[1]);
     }
     return obj[key];
 }

function isWeiXin(){ 
  var ua = window.navigator.userAgent.toLowerCase(); 
  if(ua.match(/MicroMessenger/i) == 'micromessenger'){ 
    return true; 
  }else{ 
    return false; 
  } 
} 
function removeLoading(){
  var loadingMp3 = document.getElementById('loading-mp3');
  var loading = document.getElementById('loading');
  loading.remove();
  loadingMp3.pause();
}
  if(isWeiXin()){
    document.addEventListener("WeixinJSBridgeReady", function () {
      
      document.getElementById('loading-mp3').play();
      document.getElementById('video').play()
      document.getElementById('video').pause();
      document.getElementById('loop-mp3').play();
      document.getElementById('loop-mp3').pause();

      $('.countdown').each(function()
      {
          $(this).data('from',0);
          $(this).data('to',100);
          $(this).countTo({
            onComplete:function(){
              $('.countdown').remove();
              $('#loading').append('<div class="button"></button>');
              var video = document.getElementById('video');
              video.onended = function(){
                $('#card').show();
                $('video').remove();
                $('#gif').show();
                document.getElementById('loop-mp3').play();
                $('#dowebok').fullpage({
                  onLeave:function(index,nextIndex){
                    if(!window.__rendered&&nextIndex==2){
                      window.__rendered = true;
                      $('#name').text(getSearchString('for'))
                      setTimeout(function(){
                        var invitation = $('#invitation');
                        $("#cover").show();
                        invitation.show().animate({
                          'top':'50%'
                        },function(){
                          var card = document.getElementById('card');
                          html2canvas(invitation[0], {
                            onrendered: function(canvas) {
                                var image = new Image();
                                image.src = canvas.toDataURL("image/png");
                                card.appendChild(image);
                            },
                            width: invitation.width()*2,
                            height: invitation.height()*2,
                            allowTaint: true,
                            taintTest: false
                          });
                        });
                        
                      },1000)
                    }
                  }
                });
              }
              removeLoading();
              video.play();
            }
          });
      });

      
    }, false);
    
  }else{
    document.getElementById('loading-mp3').play();
    document.getElementById('video').play()
    document.getElementById('video').pause();
    document.getElementById('loop-mp3').play();
    document.getElementById('loop-mp3').pause();

    $('.countdown').each(function()
    {
        $(this).data('from',0);
        $(this).data('to',100);
        $(this).countTo({
          onComplete:function(){
            $('.countdown').remove();
            $('#loading').append('<div class="button"></button>');
            var video = document.getElementById('video');
            video.onended = function(){
              $('#card').show();
              $('video').remove();
              $('#gif').show();
              document.getElementById('loop-mp3').play();
              $('#dowebok').fullpage({
                onLeave:function(index,nextIndex){
                  if(!window.__rendered&&nextIndex==2){
                    window.__rendered = true;
                    $('#name').text(getSearchString('for'))
                    setTimeout(function(){
                      var invitation = $('#invitation');
                      $("#cover").show();
                      invitation.animate({
                        'top':'50%'
                      },function(){
                        var card = document.getElementById('card');
                        html2canvas(invitation[0], {
                          onrendered: function(canvas) {
                              var image = new Image();
                              image.src = canvas.toDataURL("image/png");
                              card.appendChild(image);
                          },
                          width: invitation.width()*2,
                          height: invitation.height()*2,
                          allowTaint: true,
                          taintTest: false
                        });
                      });
                      
                    },1000)
                  }
                }
              });
            }
            removeLoading();
            video.play();
          }
        });
    });
  }
</script>

  <script type="text/javascript" src="http://51wzn.oss-cn-beijing.aliyuncs.com/html2canvas.js"></script>
</body>
