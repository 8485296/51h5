    <script>
        function getUrl(){
            var val = $('#input').val();encodeURIComponent
            $('#url').text('http://wetist.com?for='+encodeURIComponent(val));

            U51Wechat.init({
                app: 'rp'         // jsapilist默认会注册 ["onMenuShareTimeline", "onMenuShareAppMessage"] ， 如需新增可覆盖
                }, function (err, wx) {
                
                    if (!err) {
                        // wx 对象即官方文档上面的对象，可调用其方法
                        wx.onMenuShareAppMessage({
                            title: '51信用卡五周年邀请函', // 分享标题
